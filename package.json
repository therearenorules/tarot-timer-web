{
  "name": "tarot-timer-learn-card-meanings",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "dev": "expo start --port 8083",
    "dev-check": "node .claude/check-status.js && expo start --port 8083",
    "build": "expo build",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "build:all": "eas build --platform all",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "status": "node .claude/check-status.js",
    "build:prod": "eas build --platform all --profile production",
    "build:prod:ios": "eas build --platform ios --profile production-ios",
    "build:prod:android": "eas build --platform android --profile production-android",
    "deploy": "npm run build:prod && npm run submit:all",
    "deploy:ios": "npm run build:prod:ios && npm run submit:ios",
    "deploy:android": "npm run build:prod:android && npm run submit:android",
    "submit:all": "eas submit --platform all",
    "update": "eas update --channel production",
    "update:staging": "eas update --channel staging",
    "preview": "eas build --platform all --profile preview",
    "preview:ios": "eas build --platform ios --profile preview",
    "preview:android": "eas build --platform android --profile preview",
    "credentials": "eas credentials",
    "credentials:ios": "eas credentials --platform ios",
    "credentials:android": "eas credentials --platform android",
    "build:list": "eas build:list",
    "build:cancel": "eas build:cancel",
    "doctor": "npx expo-doctor",
    "clean": "expo r -c",
    "clean:cache": "expo r -c && npm cache clean --force",
    "prod:env": "cp .env.production .env",
    "dev:env": "cp .env.development .env",
    "check:build": "npm run doctor && npm run status",
    "app-store:prepare": "npm run prod:env && npm run check:build",
    "app-store:build": "npm run app-store:prepare && npm run build:prod",
    "app-store:submit": "npm run submit:all",
    "app-store:full": "npm run app-store:build && npm run app-store:submit",
    "deploy:manual": "node scripts/deploy.js",
    "deploy:quick": "npm run prod:env && eas build --platform all --profile production",
    "deploy:emergency": "npm run deploy:quick && npm run submit:all"
  },
  "dependencies": {
    "@expo-google-fonts/noto-sans-kr": "^0.4.2",
    "@expo/metro-runtime": "~6.1.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@supabase/supabase-js": "^2.57.4",
    "expo": "^54.0.12",
    "expo-constants": "^18.0.9",
    "expo-device": "~8.0.8",
    "expo-font": "^14.0.8",
    "expo-linear-gradient": "^15.0.7",
    "expo-notifications": "~0.32.11",
    "expo-status-bar": "~3.0.8",
    "expo-updates": "~29.0.11",
    "i18next": "^25.5.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^15.7.3",
    "react-native": "0.81.4",
    "react-native-google-mobile-ads": "^15.8.1",
    "react-native-iap": "^14.3.2",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-svg": "15.12.1",
    "react-native-svg-web": "^1.0.9",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@types/react": "~19.2.2",
    "@types/react-dom": "~19.2.1",
    "typescript": "~5.9.3"
  },
  "description": "React Native + Expo 기반의 24시간 타로 타이머 애플리케이션",
  "main": "node_modules/expo/AppEntry.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/therearenorules/tarot-timer-web.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/therearenorules/tarot-timer-web/issues"
  },
  "homepage": "https://github.com/therearenorules/tarot-timer-web#readme"
}
